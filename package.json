{
	"name": "ato-ui",
	"version": "0.4.0",
	"description": "The elemental UI component library for Svelte / SvelteKit, built with UnoCSS and Melt UI.",
	"author": "Benedikt Mielke",
	"homepage": "https://ato-ui.vercel.app/",
	"scripts": {
		"dev": "tsx ./scripts/api-docs.ts && vite dev",
		"build": "vite build && npm run package",
		"create-api-docs": "tsx ./scripts/api-docs.ts",
		"create-highlights": "tsx ./scripts/prepare.ts",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"release": "changeset publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"lint:write": "prettier --write . && eslint . --fix",
		"format": "prettier --write ."
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"typescript",
		"unocss",
		"unocss-preset",
		"tailwindcss",
		"tailwind",
		"windicss",
		"components",
		"ui",
		"headless",
		"component-library",
		"themes"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/bennymi/ato-ui.git"
	},
	"bugs": {
		"url": "https://github.com/bennymi/ato-ui/issues"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./preset": {
			"types": "./dist/preset/index.d.ts",
			"default": "./dist/preset/index.js"
		},
		"./themes/*": "./dist/styles/themes/*"
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"preset": [
				"./dist/preset/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": ">=4"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@huntabyte/mdsvex": "^0.16.8",
		"@iconify-json/logos": "^1.1.42",
		"@iconify-json/material-symbols": "^1.1.70",
		"@iconify-json/mdi": "^1.1.64",
		"@iconify-json/svg-spinners": "^1.1.2",
		"@iconify-json/vscode-icons": "^1.1.33",
		"@julr/unocss-preset-forms": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.30.3",
		"@sveltejs/package": "^2.2.6",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.11.5",
		"@types/svelte-range-slider-pips": "^2.0.4",
		"@types/tinycolor2": "^1.4.6",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"@unocss/core": "^0.58.3",
		"@unocss/extractor-svelte": "^0.58.3",
		"@unocss/preset-icons": "^0.58.3",
		"@unocss/reset": "^0.58.3",
		"@unocss/transformer-variant-group": "^0.58.3",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-svelte": "^2.33.2",
		"fast-glob": "^3.3.2",
		"fs-extra": "^11.2.0",
		"hast-util-to-html": "^9.0.0",
		"mdsvex": "^0.11.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"publint": "^0.2.2",
		"rehype-pretty-code": "^0.12.6",
		"shiki": "^0.14.7",
		"shiki-es": "^0.14.0",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-local-storage-store": "^0.6.4",
		"svelte-range-slider-pips": "^2.3.0",
		"tinycolor2": "^1.6.0",
		"tslib": "^2.6.2",
		"tsx": "^3.14.0",
		"typeit": "^8.8.3",
		"typescript": "^5.3.3",
		"unified": "^11.0.4",
		"unist-util-visit": "^5.0.0",
		"vite": "^4.5.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@melt-ui/svelte": "^0.70.0",
		"@unocss/preset-wind": "^0.58.3",
		"@unocss/transformer-directives": "^0.58.3",
		"rehype-stringify": "^10.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.0",
		"shikiji": "^0.9.0",
		"unocss": "^0.58.3"
	}
}
