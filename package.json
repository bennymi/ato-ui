{
	"name": "ato-ui",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@iconify-json/material-symbols": "^1.1.37",
		"@iconify-json/mdi": "^1.1.50",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.2",
		"@sveltejs/package": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@unocss/preset-icons": "^0.51.2",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"publint": "^0.1.11",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-headlessui": "^0.0.15",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"unocss": "^0.51.2",
		"vite": "^4.2.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@unocss/autocomplete": "^0.51.2",
		"@unocss/core": "^0.51.2",
		"@unocss/extractor-svelte": "^0.51.2",
		"@unocss/preset-attributify": "^0.51.2",
		"@unocss/preset-uno": "^0.51.2",
		"@unocss/preset-wind": "^0.51.2",
		"@unocss/reset": "^0.51.2",
		"@unocss/transformer-directives": "^0.51.2"
	}
}
